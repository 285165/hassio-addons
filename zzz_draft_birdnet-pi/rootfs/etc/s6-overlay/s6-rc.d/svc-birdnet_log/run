#!/bin/execlineb -P
with-contenv
s6-envdir /run/user/1000
s6-setuidgid ${USER}
/usr/local/bin/gotty --address localhost -p 8080 --path log --title-format "BirdNET-Pi Log" birdnet_log.sh

