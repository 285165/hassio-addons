{
    "name": "nzbget",
    "version": "0.0.4",
    "slug": "nzbget",
    "description": "Usenet downloader",
    "url": "https://github.com/petersendev/hassio-addons/nzbget",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armhf"
    ],
    "ports": {
      "6789/tcp": 6789
    },
    "ports_description": {
      "6789/tcp": "web interface"
    },
    "map": [
      "config:rw",
      "share:rw",
      "ssl"
    ],
    "webui": "http://[HOST]:[PORT:6789]",
    "boot": "auto",
    "environment": {
      "PUID": "1000",
      "PGID": "1000"
    },
    "options": {},
    "schema": {},
    "image": "petersendev/hassio-nzbget-{arch}"
  }