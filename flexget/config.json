{
  "name": "Flexget",
  "version": "3.1.132",
  "upstream": "3.1.132",
  "slug": "flexget",
  "description": "FlexGet is a multipurpose automation tool for all of your media",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "webui": "http://[HOST]:[PORT:5050]",
