    "5050/tcp": "Web interface"
  },
  "map": [
    "share:rw",
    "config:rw"
  ],
  "environment": {
    "FG_PLUGINS": "",
    "FG_WEBUI_PASSWD": ""
  },
  "options": {
    "PUID": 0,
    "PGID": 0
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "WebuiPass": "str?",
    "FG_PLUGINS": "str?"
  },
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ]
}
