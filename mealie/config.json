{
  "name": "Mealie",
  "version": "0.5.2-3",
  "upstream": "0.5.2",
  "slug": "mealie",
  "description": "Mealie is a self hosted recipe manager and meal planner with a RestAPI backend and a reactive frontend application built in Vue for a pleasant user experience for the whole family",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:9925]",
  "startup": "services",
  "boot": "auto",
  "ingress": false,
  "ingress_port": 8099,
  "panel_icon": "mdi:silverware-fork-knife",
  "ports": {
    "80/tcp": 9925
  },
  "ports_description": {
    "80/tcp": "Web interface"
  },
  "map": [
    "share:rw", 
    "ssl:r"
  ],
  "environment": {
    "DB_ENGINE": "sqlite"
  },
  "options": {},
  "schema": {
    "ssl": "bool?",
    "certfile": "match(^/ssl/.+)?",
    "keyfile": "match(^/ssl/.+)?"
    },
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ]
}
