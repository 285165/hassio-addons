{
  "name": "Nextcloud OCR",
  "url": "https://github.com/alexbelgium/hassio_addons",
  "version": "20.0.5-ls113",
  "slug": "nextcloud_ocr",
  "ingress": false,
  "ingress_port": 0,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "description": "Nextcloud for Home Assistant",
  "startup": "system",
  "boot": "manual",
  "auto_uart": true,
  "map": [
    "share:rw",
    "media:rw",
    "ssl:rw"
  ],
  "ports": {
    "443/tcp": 8099,
	"80/tcp": null
  },
  "webui": "https://[HOST]:[PORT:443]",
  "panel_icon": "mdi:cloud",
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  }
}
