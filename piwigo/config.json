{
    "name": "Piwigo",
    "version": "11.3.0",
    "slug": "piwigo",
    "description": "PiwigoÂ is a photo gallery software for the web",
    "url": "https://github.com/alexbelgium/hassio-addons/tree/master/piwigo",
    "startup": "services",
    "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
   "ports": {
      "80/tcp": 81
    },
   "ports_description": {
      "80/tcp": "Web UI port"
    },
  "privileged": [
    "SYS_ADMIN"
  ],
  "full_access": true,
  "map":[
      "media:rw",
      "share:rw",
      "ssl"
  ],
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "boot": "auto", 
  "environment": {
  "PUID": 1000,
  "PGID": 1000
  },
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem", 
    "keyfile": "privkey.pem",
    "PUID": 0,
    "PGID": 0
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str", 
    "keyfile": "str",
    "PUID": "int",
    "PGID": "int",
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?",
    "TZ": "str?"
  }
} 
