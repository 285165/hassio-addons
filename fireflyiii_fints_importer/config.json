{
  "apparmor": true,
  "arch": [
    "amd64"
  ],
  "boot": "auto",
  "description": "Import financial transactions from you FinTS enabled bank into Firefly III",
  "devices": [],
  "environment": {
    "TRUSTED_PROXIES": "**"
  },
  "image": "ghcr.io/alexbelgium/fireflyiii_fints_importer-{arch}",
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "name": "Firefly iii FinTS Importer",
  "options": {
    "CONFIG_LOCATION": "/config/addons_config/fireflyiii_fints_importer"
  },
  "ports": {
    "8080/tcp": 3476
  },
  "ports_description": {
    "8080/tcp": "web interface"
  },
  "schema": {
    "CONFIG_LOCATION": "str",
    "silent": "bool?"
  },
  "slug": "fireflyiii_fints_importer",
  "upstream": "1.2",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "1.2",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8080]"
}
