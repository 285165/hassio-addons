{
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "backup_exclude": [
    "**/Backups/*",
    "**/logs/*",
    "**/MediaCover/*"
  ],
  "boot": "auto",
  "description": "Companion application to Sonarr and Radarr to download subtitles",
  "devices": [
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2"
  ],
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "name": "Bazarr NAS",
  "options": {
    "PUID": 0,
    "PGID": 0
  },
  "ports": {
    "6767/tcp": 6767
  },
  "ports_description": {
    "6767/tcp": "web interface"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?"
  },
  "slug": "bazarr_nas",
  "startup": "services",
  "upstream": "1.0.0",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "1.0.0",
  "webui": "http://[HOST]:[PORT:6767]"
}
