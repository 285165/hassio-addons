{
  "name": "Papermerge",
  "version": "v2.0.0rc35-ls28",
  "upstream": "v2.0.0rc35-ls28",
  "slug": "papermerge",
  "description": "Open source document management system (DMS)",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "ports": {
    "8000/tcp": 8000
  },
  "ports_description": {
    "9000/tcp": "Web UI port (required)"
  },
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8000]",
  "boot": "auto",
  "environment": {
    "DOCKER_MODS": "linuxserver/mods:papermerge-multilangocr"
  },
  "full_access": true,
  "options": {
    "PUID": 0,
    "PGID": 0,
    "ocrlang": "eng,fra"
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "ocrlang": "str",
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?",
    "TZ": "str?"
  }
}
