{
  "name": "Scrutiny",
  "version": "27b923b5-ls10",
  "slug": "scrutiny",
  "stage": "experimental",
  "description": "Scrutiny WebUI for smartd S.M.A.R.T monitoring",
  "url": "https://hub.docker.com/r/linuxserver/scrutiny",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "ports": {
    "8080/tcp": 8085
  },
  "ports_description": {
    "8080/tcp": "Web UI port"
  },
  "devices": ["/dev/*","/dev/sda","/dev/sdb", "/dev/sda1","/dev/sdb1","/dev/mmcblk0p1"],
  "privileged": ["SYS_ADMIN", "SYS_RAWIO", "DAC_READ_SEARCH"],
  "full_access": true, 
  "udev": "true",
  "apparmor": "true",
  "map": [
  ],
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8080]",
  "boot": "auto",
  "environment": {
    "SCRUTINY_API_ENDPOINT": "http://localhost:8080",
    "SCRUTINY_WEB": "true",
    "SCRUTINY_COLLECTOR": "true"
  },
  "options": {
    "PUID": 0,
    "PGID": 0
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  }
}
