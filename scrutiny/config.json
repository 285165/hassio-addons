{
  "name": "Scrutiny",
  "version": "27b923b5-ls10",
  "slug": "scrutiny",
  "description": "Scrutiny WebUI for smartd S.M.A.R.T monitoring",
  "url": "https://hub.docker.com/r/linuxserver/scrutiny",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "ports": {
    "8080/tcp": 8085
  },
  "ports_description": {
    "8080/tcp": "Web UI port"
  },
  "privileged": [
    "SYS_ADMIN", 
    "SYS_RAWIO"
  ],
  "devices": [
  "/dev/sda:/dev/sda:rwm", 
  "/dev/sdb:/dev/sdb:rwm", 
  "/dev/nvme1n1:/dev/nvme1n1:rwm" 
  ], 
  "udev": "true",
  "full_access": "true", 
  "map": [
  ],
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8080]",
  "boot": "auto",
  "environment": {
    "SCRUTINY_API_ENDPOINT": "http://localhost:8080",
    "SCRUTINY_WEB": "true",
    "SCRUTINY_COLLECTOR": "true"
  },
  "options": {
    "PUID": 1000,
    "PGID": 1000
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  }
}
