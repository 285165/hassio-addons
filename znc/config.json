{
    "name": "znc",
    "version": "0.0.5",
    "slug": "znc",
    "description": "IRC network bouncer",
    "url": "https://github.com/petersendev/hassio-addons/znc",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armhf"
    ],
    "ports": {
      "6500/tcp": 6500,
      "6501/tcp": 6501
    },
    "ports_description": {
      "6500/tcp": "no SSL",
      "6501/tcp": "SSL"
    },
    "map": [
      "config:rw",
      "share:rw",
      "ssl"
    ],
    "webui": "http://[HOST]:[PORT:6500]",
    "boot": "auto",
    "environment": {
      "PUID": "1000",
      "PGID": "1000"
    },
    "options": {},
    "schema": {},
    "image": "petersendev/hassio-znc-{arch}"
  }