{
  "apparmor": true,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "boot": "auto",
  "description": "Code-server is VS Code running on a remote server, accessible through the browser.",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {},
  "map": [
    "backup:rw",
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "name": "VS code",
  "options": {
    "PUID": 0,
    "PGID": 0
  },
  "ports": {
    "8443/tcp": 8443
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?"
    "TZ": "str?"
  },
  "slug": "code-server",
  "startup": "services",
  "upstream": "4.0.2",
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/coder-server",
  "version": "4.0.2-2",
  "webui": "http://[HOST]:[PORT:8443]"
}
