{
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.cursorStyle": "block",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "hide-files.files": []
}
