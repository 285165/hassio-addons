{
  "name": "Ubooquity",
  "version": "2.1.2-ls58-rev2",
  "upstream": "2.1.2-ls58",
  "slug": "ubooquity",
  "description": "Free, lightweight and easy-to-use home server for your comics and ebooks",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "ports": {
    "2202/tcp": 2202,
    "2203/tcp": 2203
  },
  "ports_description": {
    "2202/tcp": "Library page (to expose)",
    "2203/tcp": "Admin page (local)"
  },
  "apparmor": true,
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "map": [
    "media:rw",
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "[PROTO:ssl]://[HOST]:[PORT:2203]/ubooquity/admin",
  "boot": "auto",
  "environment": {},
  "options": {
    "PUID": 0,
    "PGID": 0,
    "maxmem": 200,
    "networkdisks": "//IP/SHARE",
    "cifsusername": "username",
    "cifspassword": "folder",
    "smbv1": "false"
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "maxmem": "int",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?",
    "smbv1": "bool",
    "TZ": "str?"
  }
}
